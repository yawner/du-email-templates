            border-top: 2px solid #0048a5;
            margin-bottom: 24px;
            padding: 54px 0 48px;
        }

        .logo {
            padding-left: 24px;
        }

        .logo-wrapper {
            padding-right: 72px;
        }

        .events-header {
            padding-bottom: 36px;
        }

